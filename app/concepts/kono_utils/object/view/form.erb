<%= simple_form_for(*forms_options) do |f| %>

  <div class="row">
    <% form_attributes.each do |field| %>

      <div class="<%= cell_column_class(field) %>">
        <%= editing_form_print_field(f, field) %>
      </div>

    <% end %>
  </div>

  <%= concept(base_class.concept_ns("cell/forms/submit"), f) %>

<% end %>